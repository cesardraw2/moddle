{
  "name": "moddle",
  "version": "1.0.0",
  "description": "A library for importing meta-model based file formats into JS",
  "main": "dist/bundle.js",
  "module": "lib/index.js",
  "scripts": {
    "all": "npm run lint && npm run test && npm run bundle",
    "bundle": "NODE_ENV=production webpack",
    "dev": "npm run test -- --watch",
    "lint": "grunt lint",
    "prepublishOnly": "npm run bundle",
    "test": "mocha-webpack 'test/spec/*.js' -r test/expect.js -c  --full-trace test --webpack-config webpack.config.js test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/moddle"
  },
  "keywords": [
    "model",
    "meta-model",
    "xml",
    "xsd",
    "import",
    "export"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "eslint": "^3.19.0",
    "eslint-plugin-mocha": "^4.11.0",
    "grunt": "~0.4.2",
    "grunt-cli": "^1.2.0",
    "grunt-eslint": "^19.0.0",
    "grunt-release": "^0.7.0",
    "load-grunt-tasks": "~0.3.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.0-rc.1",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.4"
  }
}
