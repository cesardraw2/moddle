{
  "name": "moddle",
  "version": "1.0.0",
  "description": "A library for importing meta-model based file formats into JS",
  "main": "dist/bundle.js",
  "module": "lib/index.js",
  "scripts": {
    "all": "run-s lint test bundle",
    "bundle": "NODE_ENV=production webpack",
    "dev": "npm test -- --watch",
    "lint": "eslint 'lib/**/*.js' 'test/**/*.js'",
    "prepublishOnly": "run-s bundle",
    "test": "mocha --full-trace"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/moddle"
  },
  "keywords": [
    "model",
    "meta-model",
    "xml",
    "xsd",
    "import",
    "export"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "source-map-support": "^0.5.0",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.4"
  }
}
